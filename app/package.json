{
  "name": "odtd",
  "description": "ObservatÃ³rio do Trabalho Decente",
  "author": "MPT/SPAI",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env node server",
    "test": "jest",
    "stg": "cross-env node server",
    "start": "cross-env node server",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env webpack --config build/webpack.server.config.js --progress --hide-modules"
  },
  "engines": {
    "node": ">=12.11",
    "npm": ">=6.11"
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter",
    "coverageReporters": [
      "text",
      "cobertura",
      "lcov"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "collectCoverage": true,
    "coverageDirectory": "test/coverage",
    "collectCoverageFrom": [
      "assets/**/*.{js,jsx,vue}",
      "components/**/*.{js,jsx,vue}",
      "pages/**/*.{js,jsx,vue}",
      "mixins/**/*.{js,jsx,vue}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/test/**"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "\\.(yaml)$": "<rootDir>/test/__mock__/yamlMock.js"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  },
  "jestSonar": {
    "reportPath": "test",
    "reportFile": "report.xml"
  },
  "jest-junit": {
    "output": "test/coverage/junit/junit.xml",
    "usePathForSuiteName": "true"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "1.1.8",
    "@fortawesome/fontawesome-free-brands": "5.0.13",
    "@fortawesome/fontawesome-free-solid": "5.0.13",
    "@fortawesome/fontawesome-svg-core": "1.2.4",
    "@fortawesome/vue-fontawesome": "0.1.1",
    "ajv": "6.5.3",
    "axios": "0.19.0",
    "babel-polyfill": "6.26.0",
    "compression": "1.7.3",
    "cross-env": "5.0.0",
    "d3": "5.7.0",
    "d3-sankey": "0.12.1",
    "d3-scale-chromatic": "1.3.3",
    "d3plus": "2.0.0-alpha.17",
    "d3v4-bullet": "1.0.6",
    "es6-promise": "4.2.4",
    "express": "4.16.3",
    "extract-text-webpack-plugin": "3.0.2",
    "fs": "0.0.1-security",
    "json-loader": "0.5.7",
    "json2csv": "4.4.1",
    "leaflet": "1.3.4",
    "leaflet-easyprint": "2.1.9",
    "leaflet.heat": "0.2.0",
    "leaflet.markercluster": "1.4.0",
    "lru-cache": "4.1.3",
    "odometer": "0.4.8",
    "path": "0.12.7",
    "serialize-javascript": "1.5.0",
    "serve-favicon": "2.5.0",
    "uglifyjs-webpack-plugin": "1.3.0",
    "vue": "2.5.17",
    "vue-cookies": "1.5.12",
    "vue-router": "3.0.1",
    "vue-server-renderer": "2.5.17",
    "vue-svg-sprite": "1.2.3",
    "vuetify": "1.3.5",
    "vue-analytics": "5.16.2",
    "vuex": "3.0.1",
    "vuex-router-sync": "5.0.0",
    "yaml-loader": "0.5.0"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.25",
    "autoprefixer": "9.1.5",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "css-loader": "3.2.0",
    "eventsource-polyfill": "0.9.6",
    "file-loader": "0.11.1",
    "friendly-errors-webpack-plugin": "1.7.0",
    "highlight.js": "9.12.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "^23.6.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-sonar-reporter": "^2.0.0",
    "jest-junit": "^5.2.0",
    "pug": "2.0.3",
    "rimraf": "2.6.2",
    "script-ext-html-webpack-plugin": "2.0.1",
    "style-loader": "0.23.0",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.2",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "0.6.2",
    "vue-jest": "^3.0.0",
    "vue-loader": "12.2.1",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.5.17",
    "vue-worker": "1.2.1",
    "webpack": "3.12.0",
    "webpack-dev-middleware": "2.0.6",
    "webpack-hot-middleware": "2.23.1",
    "webpack-merge": "4.1.4",
    "webpack-node-externals": "1.7.2"
  }
}
